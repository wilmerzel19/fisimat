function openPanel(panel) {
	var i;
	var x = document.getElementsByClassName("panel");
	for (i = 0; i < x.length; i++) {
    x[i].style.display = "none"; 
	}
	
	document.getElementById(panel).style.display = "block"; 
	
	
	
	
	}


