function hipotenusa(b,h,p){

    var x,y;

    x= document.getElementById(b).value;
    y= document.getElementById(h).value;

    res= Math.sqrt( (x*x)+(y*y));
	
 
    document.getElementById(p).innerHTML=res;


                            }
							
